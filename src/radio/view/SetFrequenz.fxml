<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="pane" prefHeight="160.0" prefWidth="280.0" xmlns="http://javafx.com/javafx/8.0.171"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="radio.view.SetFrequenzController">
   <Label alignment="CENTER" contentDisplay="TOP" layoutY="7.0" prefHeight="32.0" prefWidth="280.0"
          text="Geben Sie Frequenz ein:" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
          AnchorPane.topAnchor="10.0">
      <font>
         <Font size="22.0"/>
      </font>
   </Label>
   <HBox alignment="CENTER" layoutX="39.0" layoutY="108.0" prefHeight="38.0" prefWidth="280.0" spacing="50.0"
         AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="110.0">
      <Button fx:id="okButton" mnemonicParsing="false" onAction="#okHandle" text="OK"/>
      <Button fx:id="cancelButton" mnemonicParsing="false" onAction="#exitHandle" text="Cancel"/>
   </HBox>
   <TextField fx:id="text" layoutX="15.0" layoutY="51.0" prefHeight="31.0" prefWidth="218.0"
              AnchorPane.leftAnchor="15.0" AnchorPane.rightAnchor="15.0" AnchorPane.topAnchor="50.0">
      <font>
         <Font size="22.0"/>
      </font>
   </TextField>
</AnchorPane>
